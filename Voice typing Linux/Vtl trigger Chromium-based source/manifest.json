{
  "manifest_version": 3,
  "name": "Voice Typing Linux Trigger",
  "version": "1.0",
  "description": "The triger for vtl. Does nothing without the app.",
  "permissions": ["nativeMessaging"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "128": "icon.png"
  }
}
